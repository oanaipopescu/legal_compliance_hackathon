@prefix : <http://example.org#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Article24 a :RegulationRule ;
    rdfs:label "Article 24" ;
    rdfs:comment "Supervisory and enforcement measures in relation to key entities (Article 32 of Directive (EU) 2022/2555)" ;
    :appliesTo :KeyEntities ;
    :references :Article32_Directive2022_2555 ;
    :requires :EffectiveProportionateDissuasiveMeasures .

:Article32_Directive2022_2555 a :LegalBasis ;
    rdfs:label "Article 32 of Directive (EU) 2022/2555" .

:EffectiveProportionateDissuasiveMeasures a :Obligation ;
    rdfs:label "Effective, Proportionate and Dissuasive Measures" .

:NationalCybersecurityAuthority a :LegalEntity ;
    rdfs:label "National Cybersecurity Authority" ;
    :hasCompetence :OnSiteInspectionsSupervision, :RegularTargetedSecurityAudits, :ExtraordinarySpecialInspections, :SecurityScans, :InformationRequests, :DataDocumentInformationAccessRequests, :EvidenceRequests ;
    :responsibilities :IssueWarningsRecommendations, :IssueBindingInstructionsGuidelines, :OrderCessationOfConduct, :OrderComplianceWithArticles15_16, :InstructInformationToAffectedPersons, :InstructImplementationOfRecommendations, :AppointCompetentSupervisor, :OrderPublicInformationOnViolations, :ImposeAdministrativeFines .

:OnSiteInspectionsSupervision a :Competence ;
    rdfs:label "On-site inspections and supervision" ;
    :carriedOutBy :InspectorsOfNCSA .

:InspectorsOfNCSA a :LegalEntityClass ;
    rdfs:label "Inspectors of the National Cybersecurity Authority" ;
    :definedIn :Article23 .

:RegularTargetedSecurityAudits a :Competence ;
    rdfs:label "Regular and Targeted Security Audits" ;
    :basedOn :RiskAssessments .

:RiskAssessments a :LegalBasis ;
    rdfs:label "Risk Assessments" .

:ExtraordinarySpecialInspections a :Competence ;
    rdfs:label "Extraordinary Special Inspections" ;
    :justifiedBy :SignificantIncidentOrViolation .

:SignificantIncidentOrViolation a :Condition ;
    rdfs:label "Significant Incident or Violation of this law" .

:SecurityScans a :Competence ;
    rdfs:label "Security Scans" ;
    :requires :ObjectiveImpartialFairTransparentRiskAssessmentCriteria .

:ObjectiveImpartialFairTransparentRiskAssessmentCriteria a :Obligation ;
    rdfs:label "Objective, Impartial, Fair and Transparent Risk Assessment Criteria" .

:InformationRequests a :Competence ;
    rdfs:label "Requests for Necessary Information" .

:DataDocumentInformationAccessRequests a :Competence ;
    rdfs:label "Requests for Access to Data, Documents and Information" .

:EvidenceRequests a :Competence ;
    rdfs:label "Requests for Evidence" .

:IssueWarningsRecommendations a :Responsibility ;
    rdfs:label "Issue warnings or recommendations" .

:IssueBindingInstructionsGuidelines a :Responsibility ;
    rdfs:label "Issue binding instructions and guidelines" .

:OrderCessationOfConduct a :Responsibility ;
    rdfs:label "Order cessation of conduct" .

:OrderComplianceWithArticles15_16 a :Responsibility ;
    rdfs:label "Order compliance with Articles 15 and 16" .

:InstructInformationToAffectedPersons a :Responsibility ;
    rdfs:label "Instruct relevant entities to inform affected persons" .

:InstructImplementationOfRecommendations a :Responsibility ;
    rdfs:label "Instruct implementation of recommendations" .

:AppointCompetentSupervisor a :Responsibility ;
    rdfs:label "Appoint a competent supervisor" .

:OrderPublicInformationOnViolations a :Responsibility ;
    rdfs:label "Order public information on violations" .

:ImposeAdministrativeFines a :Responsibility ;
    rdfs:label "Impose administrative fines" ;
    :basedOn :Article26 .

:Article26 a :RegulationRule ;
    rdfs:label "Article 26" .

:IneffectiveEnforcementMeasures a :Condition ;
    rdfs:label "Enforcement Measures Deemed Ineffective" ;
    :resultsIn :TemporarySuspensionOrProhibition .

:TemporarySuspensionOrProhibition a :Sanction ;
    rdfs:label "Temporary Suspension or Prohibition" .

:KeyEntities a :LegalEntityClass ;
    rdfs:label "Key Entities" ;
    :definedIn :Article3 .

:Article3 a :RegulationRule ;
    rdfs:label "Article 3".

:NaturalPersonLiability a :Obligation ;
    rdfs:label "Natural Person Liability for Key Entity Non-Compliance" .

:EnforcementMeasureConsiderations a :Obligation ;
    rdfs:label "Considerations for Enforcement Measures" ;
    :concerns :SeriousnessOfInfringement, :DurationOfInfringement, :RelevantPreviousInfringements, :MaterialOrNonMaterialDamage, :FaultOnPartOfPerpetrator, :MeasuresTakenToPreventMitigateDamage, :AdherenceToApprovedCodesCertification, :DegreeOfCooperation .

:SeriousnessOfInfringement a :Criterion ;
    rdfs:label "Seriousness of the infringement" .

:DurationOfInfringement a :Criterion ;
    rdfs:label "Duration of the infringement" .

:RelevantPreviousInfringements a :Criterion ;
    rdfs:label "Relevant previous infringements" .

:MaterialOrNonMaterialDamage a :Criterion ;
    rdfs:label "Material or non-material damage caused" .

:FaultOnPartOfPerpetrator a :Criterion ;
    rdfs:label "Fault on the part of the perpetrator" .

:MeasuresTakenToPreventMitigateDamage a :Criterion ;
    rdfs:label "Measures taken to prevent or mitigate damage" .

:AdherenceToApprovedCodesCertification a :Criterion ;
    rdfs:label "Adherence to approved codes or certification" .

:DegreeOfCooperation a :Criterion ;
    rdfs:label "Degree of cooperation with authorities" .

:EnforcementMeasureNotification a :Obligation ;
    rdfs:label "Notification of Enforcement Measures" .

:CooperationWithAuthorities a :Obligation ;
    rdfs:label "Cooperation with other Authorities" ;
    :concerns :Directive2022_2557, :Regulation2022_2554 .

:Directive2022_2557 a :EU_Directive ;
    rdfs:label "Directive (EU) 2022/2557".

:Regulation2022_2554 a :EU_Regulation ;
    rdfs:label "Regulation (EU) 2022/2554".