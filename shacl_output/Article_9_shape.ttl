```turtle
@prefix : <http://example.org#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

:LargeScaleIncidentCrisisManagementPlanShape a sh:NodeShape ;
    sh:targetClass :LargeScaleIncidentCrisisManagementPlan ;
    sh:property [
        sh:path :requires ;
        sh:minCount 6 ;
        sh:message "A national plan for responding to large-scale incidents must include at least 6 requirements" ;
        sh:severity sh:Violation
    ] .

:NationalPreparednessObjectivesShape a sh:NodeShape ;
    sh:targetClass :NationalPreparednessObjectives ;
    sh:property [
        sh:path rdfs:label ;
        sh:minCount 1 ;
        sh:message "Objectives of national preparedness measures must be defined"
    ] .

:NCSAuthorityDutiesResponsibilitiesShape a sh:NodeShape ;
    sh:targetClass :NCSAuthorityDutiesResponsibilities ;
    sh:property [
        sh:path rdfs:label ;
        sh:minCount 1 ;
        sh:message "Duties and responsibilities of the National Cybersecurity Authority must be defined"
    ] .

:CyberCrisisManagementProceduresShape a sh:NodeShape ;
    sh:targetClass :CyberCrisisManagementProcedures ;
    sh:property [
        sh:path rdfs:label ;
        sh:minCount 1 ;
        sh:message "Cyber crisis management procedures must be defined"
    ] .

:NationalPreparednessMeasuresShape a sh:NodeShape ;
    sh:targetClass :NationalPreparednessMeasures ;
    sh:property [
        sh:path rdfs:label ;
        sh:minCount 1 ;
        sh:message "National preparedness measures must be defined"
    ] .

:StakeholderInfrastructureIdentificationShape a sh:NodeShape ;
    sh:targetClass :StakeholderInfrastructureIdentification ;
    sh:property [
        sh:path rdfs:label ;
        sh:minCount 1 ;
        sh:message "Stakeholder infrastructure identification must be defined"
    ] .

:EULevelCoordinationProceduresShape a sh:NodeShape ;
    sh:targetClass :EULevelCoordinationProcedures ;
    sh:property [
        sh:path rdfs:label ;
        sh:minCount 1 ;
        sh:message "EU-level coordination procedures must be defined"
    ] .

:NotificationToEuropeanCommissionShape a sh:NodeShape ;
    sh:targetClass :NotificationToEuropeanCommission ;
    sh:property [
        sh:path :requires ;
        sh:minCount 2 ;
        sh:message "Notification to the European Commission requires at least two information elements"
    ] .

```