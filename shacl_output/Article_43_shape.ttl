```turtle
@prefix : <http://example.org#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:RegulationRuleShape a sh:NodeShape ;
    sh:targetClass :RegulationRule ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Regulation Rule must have a label" ] ;
    sh:property [ sh:path :subject ; sh:minCount 1 ; sh:message "Regulation Rule must have a subject" ] ;
    sh:property [ sh:path :requires ; sh:minCount 1 ; sh:message "Regulation Rule must require at least one obligation" ] .

:LicenseShape a sh:NodeShape ;
    sh:targetClass :License ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "License must have a label" ] ;
    sh:property [ sh:path :grantedBy ; sh:minCount 1 ; sh:message "License must specify granting authority" ] ;
    sh:property [ sh:path :requires ; sh:minCount 1 ; sh:message "License must specify requirements" ] .

:LegalEntityShape a sh:NodeShape ;
    sh:targetClass :LegalEntity ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Legal Entity must have a label" ] .

:CertificateShape a sh:NodeShape ;
    sh:targetClass :Certificate ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Certificate must have a label" ] .

:DocumentShape a sh:NodeShape ;
    sh:targetClass :Document ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Document must have a label" ] .

:PermitShape a sh:NodeShape ;
    sh:targetClass :Permit ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Permit must have a label" ] .

:RoleShape a sh:NodeShape ;
    sh:targetClass :Role ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Role must have a label" ] .

:ScopeShape a sh:NodeShape ;
    sh:targetClass :Scope ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Scope must have a label" ] .

:LegalEntityClassShape a sh:NodeShape ;
    sh:targetClass :LegalEntityClass ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Legal Entity Class must have a label" ] .
