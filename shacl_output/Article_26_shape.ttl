```turtle
@prefix : <http://example.org#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:RegulationRuleShape a sh:NodeShape ;
    sh:targetClass :RegulationRule ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Regulation Rule must have a label" ] ;
    sh:property [ sh:path :appliesTo ; sh:minCount 1 ; sh:message "Regulation Rule must specify what it applies to" ] ;
    sh:property [ sh:path :references ; sh:message "Regulation Rule may specify referenced legislation (optional)" ] ;
    sh:property [ sh:path :requires ; sh:minCount 1 ; sh:message "Regulation Rule must require at least one obligation" ] .

:LegalBasisShape a sh:NodeShape ;
    sh:targetClass :LegalBasis ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Legal Basis must have a label" ] ;
    sh:property [ sh:path :concerns ; sh:minCount 1 ; sh:message "Legal Basis must specify what it concerns" ] .

:ObligationShape a sh:NodeShape ;
    sh:targetClass :Obligation ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Obligation must have a label" ] ;
    sh:property [ sh:path :appliesTo ; sh:message "Obligation may specify what it applies to (optional)" ] .

:LegalEntityShape a sh:NodeShape ;
    sh:targetClass :LegalEntity ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Legal Entity must have a label" ] ;
    sh:property [ sh:path :imposes ; sh:message "Legal Entity may impose sanctions (optional)" ] .

:SanctionShape a sh:NodeShape ;
    sh:targetClass :Sanction ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Sanction must have a label" ] ;
    sh:property [ sh:path :maximumAmount ; sh:message "Sanction may have a maximum amount (optional)" ] ;
    sh:property [ sh:path :basedOn ; sh:message "Sanction may be based on legal basis (optional)" ] ;
    sh:property [ sh:path :imposedBy ; sh:message "Sanction may specify who imposes it (optional)" ] .

:MonetaryValueShape a sh:NodeShape ;
    sh:targetClass :MonetaryValue ;
    sh:property [ sh:path rdfs:label ; sh:datatype xsd:decimal ; sh:minCount 1 ; sh:message "Monetary Value must have a label and be a decimal" ] .

:LegalProcessShape a sh:NodeShape;
    sh:targetClass :LegalProcess;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Legal Process must have a label" ] ;
    sh:property [ sh:path :concerns ; sh:minCount 1 ; sh:message "Legal Process must specify what it concerns" ] .

:InformationElementShape a sh:NodeShape ;
    sh:targetClass :InformationElement ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Information Element must have a label" ] ;
    sh:property [ sh:path :concerns ; sh:message "Information Element may specify what it concerns (optional)" ] .

:WebsiteShape a sh:NodeShape;
    sh:targetClass :Website;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Website must have a label" ].

:LegalEntityClassShape a sh:NodeShape ;
    sh:targetClass :LegalEntityClass ;
    sh:property [ sh:path rdfs:label ; sh:minCount 1 ; sh:message "Legal Entity Class must have a label" ] .

```