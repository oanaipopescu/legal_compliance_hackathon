@prefix : <http://example.org#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AccurateCompleteDomainNameRegistrationDataShape a sh:NodeShape ;
    sh:targetClass :DomainNameRegistrationDatabase ;
    sh:property [
        sh:path :contains ;
        sh:minCount 6 ;
        sh:message "Domain Name Registration Database must contain at least six data elements." ;
        sh:severity sh:Violation
    ] .

:TimelyResponseShape a sh:NodeShape ;
    sh:targetClass :AccessToSpecificDomainNameRegistrationData ;
    sh:property [
        sh:path :requires ;
        sh:hasValue :TimelyResponse ;
        sh:message "Access to specific domain name registration data requires a Timely Response." ;
        sh:severity sh:Violation
    ] .