@prefix : <http://example.org#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:VoluntaryInformationExchangeShape a sh:NodeShape ;
    sh:targetClass :Suppliers ;
    sh:property [
        sh:path :hasInformation ;
        sh:minCount 1 ;
        sh:message "Suppliers must share at least one type of cybersecurity information as defined in Article 21." ;
        sh:severity sh:Violation
    ] .

:hasInformation a rdf:Property ;
    rdfs:label "has Information" ;
    rdfs:range :InformationElement .