:Article30Shape a sh:NodeShape ;
    sh:targetClass :AuthorizingProvision ;
    sh:property [
        sh:path :requires ;
        sh:minCount 1 ;
        sh:message "Authorizing provisions must specify at least one requiring decision or opinion." ;
        sh:severity sh:Violation
    ] .

:Article30_paragraph3Shape a sh:NodeShape ;
    sh:targetClass :AuthorizingProvision ;
    sh:target :Article30_paragraph3 ;
    sh:property [
        sh:path :deadline ;
        sh:pattern "^P[0-9]+M$" ;
        sh:message "The deadline for Article 30, paragraph 3 must be specified as PnM, where n is a positive integer." ;
        sh:severity sh:Violation
    ] .

