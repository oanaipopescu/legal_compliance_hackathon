@prefix : <http://example.org#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:VoluntaryIncidentNotificationShape a sh:NodeShape ;
    sh:targetClass :LegalEntity ;
    sh:property [
        sh:path :processes ;
        sh:hasValue :VoluntaryIncidentNotification ;
        sh:message "Legal entities must process Voluntary Incident Notifications as per Article 22." ;
        sh:severity sh:Violation
    ] .