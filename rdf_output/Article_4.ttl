:IPRanges a :InformationElement ;
    rdfs:label "IP Address Ranges" .

:DomainNames a :InformationElement ;
    rdfs:label "Domain Names" .

:SectorSubsectorType a :InformationElement ;
    rdfs:label "Sector, Subsector, and Entity Type" .

:EUCountriesServed a :InformationElement ;
    rdfs:label "EU Countries Served" .

:Law4577_2018 a :NationalLaw ;
    rdfs:label "Law 4577/2018" .

:Decision1027_2019 a :NationalDecision ;
    rdfs:label "Decision No. 1027/4.10.2019" .

